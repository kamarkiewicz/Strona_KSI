{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Front-end of KSI webpage",
  "author": "Kamil Markiewicz <k.a.markiewicz@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test:unit": "jest --config test/unit/jest.conf.js --coverage",
    "test:e2e": "yarn run build && nightwatch --config test/e2e/nightwatch.conf.js --env chrome",
    "test": "yarn run test:unit && yarn run test:e2e"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.1",
    "@nuxtjs/font-awesome": "^1.0.3",
